name: Ujian Golang
services:
    mysql:
        container_name: db
        environment:
            - MYSQL_ROOT_PASSWORD=root
            - MYSQL_DATABASE=ujian
        networks:
            - mynet
        image: mysql:latest
        command: free -s 2 -c 30
    myapp:
        stdin_open: true
        tty: true
        container_name: app
        volumes:
            - ./Public/app/storage:/app/Public/app/storage:Z
        networks:
            - mynet
        image: myapp:latest
	depends_on:
		mysql
networks:
    mynet:
        external: true
        name: mynet
